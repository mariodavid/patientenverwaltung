<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/5.5/window.xsd"
        caption="msg://editCaption"
        class="de.csh.patientenverwaltung.gui.krakenkasse.KrakenkasseEdit"
        datasource="krakenkasseDs"
        focusComponent="fieldGroup"
        messagesPack="de.csh.patientenverwaltung.gui.krakenkasse">
    <dsContext>
        <datasource id="krakenkasseDs"
                    class="de.csh.patientenverwaltung.entity.Krakenkasse"
                    view="_local"/>
    </dsContext>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="krakenkasseDs">
            <column width="250px">
                <field id="name"/>
                <field id="anschrift"/>
                <field id="plz"/>
                <field id="ort"/>
                <field id="telefon"/>
                <field id="fax"/>
                <field id="email"/>
            </column>
        </fieldGroup>
        <iframe id="windowActions"
                screen="editWindowActions"/>
    </layout>
</window>
